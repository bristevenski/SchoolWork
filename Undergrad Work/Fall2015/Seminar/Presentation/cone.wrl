#VRML V2.0 utf8

Shape {
	geometry Cone {
		bottomRadius 1.0	# all fields are optional
		height 2.0			# if a field is not specified
		side TRUE			# the default value is used
		bottom TRUE
	}
	appearance Appearance {
		material Material {  
			diffuseColor 1 0 0 
			ambientIntensity 0.0 
			emissiveColor 0.0 0.0 0.0 
			specularColor 0.0 0.0 0.0 
			shininess 0.8 
			transparency 0.0 
		}

	}
}
